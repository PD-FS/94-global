<p id="notice"><%= notice %></p>

<h1>Awards</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Stock</th>
      <th>Minimumage</th>
      <th>Maximumage</th>
      <th>Picture</th>
      <th>Description</th>
      <th>Terms</th>
      <th>Startdate</th>
      <th>Enddate</th>
      <th>Rate</th>
      <th>Civicoins</th>
      <th>Enterprise</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @awards.each do |award| %>
      <tr>
        <td><%= award.name %></td>
        <td><%= award.stock %></td>
        <td><%= award.minimumAge %></td>
        <td><%= award.maximumAge %></td>
        <td><%= award.picture %></td>
        <td><%= award.description %></td>
        <td><%= award.terms %></td>
        <td><%= award.startDate %></td>
        <td><%= award.endDate %></td>
        <td><%= award.rate %></td>
        <td><%= award.civicoins %></td>
        <td><%= award.enterprise %></td>
        <td><%= link_to 'Show', award %></td>
        <td><%= link_to 'Edit', edit_award_path(award) %></td>
        <td><%= link_to 'Destroy', award, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Award', new_award_path %>
